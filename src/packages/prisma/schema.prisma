// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Template{
  id String @id @default(uuid())
  titulo String
  synopsis String
  imagen String
  size Int
  scene_size Int  
}

model Actor{
  id String @id @default(uuid())
  name String
  description String?
}